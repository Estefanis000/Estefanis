{
  "name": "portfolio-ortiz-estefanis",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cd portfolio && vite",
    "build": "cd portfolio && vite build",
    "preview": "cd portfolio && vite preview",
    "fetch-icons": "cd portfolio && node tools/fetch-icons.js"
  },
  "devDependencies": {
    "vite": "^5"
  },
{ 
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ]
}
